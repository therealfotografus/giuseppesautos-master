	<title>Giuseppe's Autos Rólunk</title>
	<link rel="icon" href="imgs/logo.png" type="image/x-icon">
	<link rel="stylesheet" href="/css/styles.css">
	
	<script src="/js/index.js" defer></script>
	<script src="/js/copy.js" defer></script>
</head>

<body class="noselect">

	<!-----Navbar---->

	<nav class="navbar navbar-expand-sm bg-dark navbar-dark m-auto p-3"
		style="z-index: 9; position: fixed; top: 0; width: 100%; transition: 0.4;">

		<a class="nav-brand active" href="./"><img id="m_logo" src="imgs/logo.png"></a>

		<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbar">
			<span class="navbar-toggler-icon"></span>
		</button>

		<div class="collapse navbar-collapse justify-content-end" id="navbar">
			<ul class="navbar-nav">
				<li class="nav-item">
					<a href="katalogus" class="nav-link"><img style="position: fixed; right: 200px;" src="imgs/katalogus.png"><br>
					<hr>
					<i>Katalógus</i>
					</a>
				</li>
                <li class="nav-item">
					<a href="partnereink" class="nav-link"><img style="position: fixed; right: 106px;" src="imgs/partnereink.png"><br>
					<hr>
					<i>Partnereink</i>
					</a>
				</li>
				<li class="nav-item">
					<a href="rolunk" class="nav-link"><img style="position: fixed; right: 33px;" src="imgs/rolunk.png"><br>
					<hr>
					<i>Rólunk</i>
					</a>
				</li>
			</ul>
		</div>
	</nav>
	<div class="dummy"></div>





	<div class="container-fluid wh-100 d-flex p-0">
		<div class="py-4 px-5 w-100">
			<div class="w-100 pb-5">
				<p class="display-3 text-center">Tulajdonos</p>
				<hr class="d-block m-auto w-75">
			</div>
			<div class="member-container">
				<% people.forEach((element, index) => {
					var name = element[5]

					if (element[5].length > 16){
						name = element[5].substring(0,16) + "..."
					}
					if ((element[2] == "Tulajdonos") && (element[7])){ %>
						<div class="pb-5">
							<div class="card" style="width: 27rem; height: 27rem;">
								<div class="card-body">
									<img class="card-img" src="<%= element[7] || "https://static1.e621.net/data/47/4c/474ced57816f56cfe7a7a7d4d6381c6e.png" %>" alt="">
									<div class="card-img-overlay hover">
										<div class="member-data w-100 h-100 text-center text-dark">
											<div>
												<span class="display-5"><%= element[1] || "Nincs neve"%></span>
												<br>
												<span class="h5"><%= element[2] || "Nincs beosztása" %></span>
												<br>
												<br>
												<span class="h4">Elérhetőségek:</span>
												<br>
												<hr class="w-75 d-block mx-auto">
												<span class="fs-4">Telefon:</span>
												<br>
												<span class="h3"><%= element[4] || "Nem elérhető" %></span>
												<br>
												<span class="fs-4">E-mail (Discord):</span>
												<br>
												<span class="h3 mail" style="cursor: pointer;" data-bs-toggle="tooltip" title="Kattintson ide a másoláshoz" onclick="copyTextToClipboard('<%= element[5] %>')"><%= element[5] %></span>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					<% }
				}) %>
			</div>
